#include<bits/stdc++.h>
using namespace std;

int floorOfElement(vector<int> &arr,int x){
    int n = arr.size();
    int l =0 ,h=n-1;
    int ans =-1;
    while(l<=h){
        int mid = l +(h-l)/2;
        if(arr[mid]>x){
            h = mid-1;
        }else{
            ans = arr[mid];
            l = mid+1;
        }
    }
    return ans;
}
int main(){
    vector<int> arr = {1,2,3,8,9,10};
    int x = 6;
    cout<<floorOfElement(arr,x);
    return 0;
}
